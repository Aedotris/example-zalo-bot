{
	"name": "Get Zalo IMEI & Cookies",
	"version": "1.0",
	"manifest_version": 2,
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": ["notifications", "activeTab", "webRequest", "webRequestBlocking", "cookies", "<all_urls>"],
	"browser_action": {
		"default_popup": "popup.html"
	}
}